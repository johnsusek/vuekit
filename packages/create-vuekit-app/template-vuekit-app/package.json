{
  "name": "vuekit-template",
  "version": "0.0.1",
  "author": "",
  "license": "",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "lint": "eslint --ext vue,ts --quiet .",
    "tsc": "vue-tsc -p ./tsconfig.json -noEmit",
    "bundle": "vite build -l error --config .config/vite.config.ts",
    "bundle:copy": "cp build/js/vuekit-bundle.es.js build/app/Build/Products/Debug/$npm_package_name.app/Contents/Resources/vuekit-bundle.es.js",
    "app": "./build/app/Build/Products/Debug/$npm_package_name.app/Contents/MacOS/$npm_package_name",
    "xcodegen:app": "xcodegenerate -s $npm_config_local_prefix/.config/xcodeproj-app.json -p $npm_config_local_prefix/build",
    "xcodegen:framework": "mkdir -p $npm_config_local_prefix/build/framework && xcodegenerate -s $npm_config_local_prefix/.config/xcodeproj-framework.json -p $npm_config_local_prefix/build",
    "xcodebuild:app": "xcodebuild -quiet -allowProvisioningUpdates -derivedDataPath build/app -scheme $npm_package_name -project build/$npm_package_name.xcodeproj",
    "xcodebuild:framework": "xcodebuild -quiet -allowProvisioningUpdates -derivedDataPath build/framework -scheme VueKit -project build/VueKit.xcodeproj",
    "build:app": "rm -rf build/app && npm run xcodegen:app && npm run xcodebuild:app",
    "build:framework": "rm -rf build/framework && npm run xcodegen:framework && npm run xcodebuild:framework",
    "build:js": "rm -rf build/js && npm run lint && npm run tsc && npm run bundle",
    "build": "npm run build:js && npm run build:framework && npm run build:app",
    "watch:app": "nodemon -w './build/js' --exec 'npm run app'",
    "watch:js": "nodemon -e 'ts,vue' -w './src' --exec 'npm run build:js && npm run bundle:copy'"
  },
  "eslintConfig": {
    "extends": ".config/.eslintrc.js"
  },
  "dependencies": {},
  "devDependencies": {
    "vuekit-vue": "0.0.*"
  }
}