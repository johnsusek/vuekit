{
  "name": "vuekit-template",
  "version": "0.0.1",
  "author": "",
  "license": "",
  "scripts": {
    "clean": "rm -rf build logs/* && mkdir build",
    "build": "npm run vue:build && mkdir -p logs && npm run framework:build && npm run app:build",
    "app": "killall $npm_package_name || true && ./build/app/Build/Products/Debug/$npm_package_name.app/Contents/MacOS/$npm_package_name",
    "app:xcodegen": "xcodegenerate -s $npm_config_local_prefix/.config/xcodeproj-app.json -p $npm_config_local_prefix/build &> logs/xcodegen-app.log",
    "app:xcodebuild": "xcodebuild -allowProvisioningUpdates -derivedDataPath build/app -scheme $npm_package_name -project build/$npm_package_name.xcodeproj &> logs/xcodebuild-app.log",
    "app:build": "rm -rf build/app && npm run app:xcodebuild && npm run app:xcodebuild",
    "app:watch": "nodemon -e js -w ./build --exec 'npm run app'",
    "framework:xcodegen": "mkdir -p $npm_config_local_prefix/build/framework && xcodegenerate -s $npm_config_local_prefix/.config/xcodeproj-framework.json -p $npm_config_local_prefix/build &> logs/xcodegen-framework.log",
    "framework:xcodebuild": "xcodebuild -allowProvisioningUpdates -derivedDataPath build/framework -scheme VueKit -project build/VueKit.xcodeproj &> logs/xcodebuild-framework.log",
    "framework:build": "rm -rf build/framework && npm run framework:xcodegen && npm run framework:xcodebuild",
    "vue:lint": "eslint --ext vue,ts --quiet .",
    "vue:tsc": "vue-tsc -p ./tsconfig.json -noEmit",
    "vue:bundle": "vite build -l error --config .config/vite.config.ts",
    "vue:bundle:copy": "cp build/js/vuekit-bundle.es.js build/app/Build/Products/Debug/$npm_package_name.app/Contents/Resources/vuekit-bundle.es.js",
    "vue:build": "npm run vue:lint && npm run vue:tsc && npm run vue:bundle",
    "vue:watch": "nodemon -e 'ts,vue' -w ./src --exec 'npm run vue:build && npm run vue:bundle:copy'"
  },
  "eslintConfig": {
    "extends": ".config/.eslintrc.js"
  },
  "dependencies": {},
  "devDependencies": {
    "vuekit-vue": "0.0.*"
  }
}