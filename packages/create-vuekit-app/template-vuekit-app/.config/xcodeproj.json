{
  "name": "my-vuekit-app",
  "options": {
    "deploymentTarget": {
      "macOS": "10.15"
    }
  },
  "settings": {
    "DEVELOPMENT_TEAM": "",
    "CODE_SIGN_STYLE": "Automatic"
  },
  "configs": {
    "Debug": "debug",
    "Release": "release",
    "Adhoc": "debug",
    "Beta": "debug"
  },
  "targets": {
    "VueKit": {
      "type": "framework",
      "platform": "macOS",
      "sources": [
        "../node_modules/vuekit-vue/framework/Sources"
      ],
      "settings": {
        "INFOPLIST_FILE": "node_modules/vuekit-vue/framework/Info.plist"
      }
    },
    "my-vuekit-app": {
      "type": "application",
      "platform": "macOS",
      "sources": [
        "../node_modules/vuekit-vue/templates/xcode/Sources",
        {
          "path": "../build/vuekit-bundle.es.js",
          "optional": true
        }
      ],
      "dependencies": [
        {
          "target": "VueKit"
        }
      ],
      "scheme": {
        "testTargets": [
          {
            "name": "my-vuekit-app-tests"
          }
        ]
      }
    },
    "my-vuekit-app-tests": {
      "type": "bundle.unit-test",
      "platform": "macOS",
      "sources": [
        "../node_modules/vuekit-vue/templates/xcode/Tests"
      ],
      "dependencies": [
        {
          "target": "my-vuekit-app"
        }
      ]
    }
  }
}