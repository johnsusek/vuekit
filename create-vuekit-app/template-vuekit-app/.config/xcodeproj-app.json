{
  "name": "VueKitApp",
  "options": {
    "deploymentTarget": {
      "macOS": "10.15"
    }
  },
  "settings": {
    "DEVELOPMENT_TEAM": "${DEVELOPMENT_TEAM}",
    "CODE_SIGN_STYLE": "Automatic"
  },
  "configs": {
    "Debug": "debug",
    "Release": "release",
    "Adhoc": "debug",
    "Beta": "debug"
  },
  "packages": {
    "VueKitBridge": {
      "github": "johnsusek/vuekit-bridge",
      "branch": "master"
    }
  },
  "targets": {
    "VueKitApp": {
      "type": "application",
      "platform": "macOS",
      "info": {
        "path": "../assets/Info.plist",
        "properties": {
          "NSMainNibFile": "MainMenu",
          "NSPrincipalClass": "NSApplication"
        }
      },
      "sources": [
        "../node_modules/vuekit-vue/templates/xcode/Sources",
        {
          "path": "../build/js/vuekit-bundle.es.js",
          "optional": true
        },
        {
          "path": "../assets"
        }
      ],
      "dependencies": [
        {
          "package": "VueKitBridge"
        },
        {
          "framework": "../build/framework/Build/Products/Debug/FrameworkBridge.framework"
        }
      ],
      "scheme": {
        "testTargets": [
          {
            "name": "VueKitAppTests"
          }
        ]
      }
    },
    "VueKitAppTests": {
      "type": "bundle.unit-test",
      "platform": "macOS",
      "sources": [
        "../node_modules/vuekit-vue/templates/xcode/Tests"
      ],
      "dependencies": [
        {
          "target": "VueKitApp"
        }
      ]
    }
  }
}