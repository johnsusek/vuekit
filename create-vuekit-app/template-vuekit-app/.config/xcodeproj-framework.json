{
  "name": "VueKit",
  "options": {
    "deploymentTarget": {
      "macOS": "10.15"
    }
  },
  "settings": {
    "DEVELOPMENT_TEAM": "",
    "CODE_SIGN_STYLE": "Automatic"
  },
  "configs": {
    "Debug": "debug",
    "Release": "release",
    "Adhoc": "debug",
    "Beta": "debug"
  },
  "targets": {
    "VueKit": {
      "type": "framework",
      "platform": "macOS",
      "sources": [
        "../node_modules/vuekit-vue/framework/Sources/vuekit/",
        {
          "path": "../node_modules/vuekit-vue/framework/Sources/exports/",
          "includes": [
            "Foundation",
            "AppKit",
            "AVFoundation",
            "AVKit",
            "CoreAnimation",
            "CoreFoundation",
            "CoreData",
            "CoreGraphics",
            "CoreImage",
            "CoreMedia",
            "ObjectiveC",
            "Quartz",
            "QuartzCore"
          ]
        }
      ],
      "settings": {
        "INFOPLIST_FILE": "../node_modules/vuekit-vue/framework/Info.plist"
      }
    }
  }
}